{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "C/C++: g++ build this project in default optimization",
			"command": "/usr/bin/g++",
			"args": [
				"-fdiagnostics-color=always",
				"-g",
				"-Wall",
				"-Werror",
				"-fopenmp",
				"-I",
				"${workspaceFolder}/Cplusplus/include/",
				"-I",
				"${workspaceFolder}/Cplusplus/include/LinkedLists/",
				"-I",
				"${workspaceFolder}/Cplusplus/include/NumericProgression/",
				"${workspaceFolder}/Cplusplus/source/LinkedLists/*.cpp",
				"${workspaceFolder}/Cplusplus/source/NumericProgression/*.cpp",
				"${workspaceFolder}/Cplusplus/source/*.cpp",
				"-o",
				"${workspaceFolder}/Cplusplus/bin/project_entry"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "compiler: /usr/bin/g++"
		},
		{
			"type": "shell",
			"label": "C/C++: g++ build this project in optimization level 3",
			"command": "/usr/bin/g++",
			"args": [
				"-fdiagnostics-color=always",
				"-O3",
				"-g",
				"-Wall",
				"-Werror",
				"-fopenmp",
				"-I",
				"${workspaceFolder}/Cplusplus/include/",
				"-I",
				"${workspaceFolder}/Cplusplus/include/LinkedLists/",
				"-I",
				"${workspaceFolder}/Cplusplus/include/NumericProgression/",
				"${workspaceFolder}/Cplusplus/source/LinkedLists/*.cpp",
				"${workspaceFolder}/Cplusplus/source/NumericProgression/*.cpp",
				"${workspaceFolder}/Cplusplus/source/*.cpp",
				"-o",
				"${workspaceFolder}/Cplusplus/bin/project_entry_O3"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": false
			},
			"detail": "compiler: /usr/bin/g++"
		},
		{
			"type": "shell",
			"label": "C/C++: g++ build the active unit test",
			"command": "/usr/bin/g++",
			"args": [
				"-fdiagnostics-color=always",
				"-g",
				"-Wall",
				"-Werror",
				"-fopenmp",
				"-I",
				"${workspaceFolder}/Cplusplus/include/",
				"-I",
				"${workspaceFolder}/Cplusplus/include/LinkedLists/",
				"-I",
				"${workspaceFolder}/Cplusplus/include/NumericProgression/",
				"-L",
				"/usr/local/include/gtest/",
				"-L",
				"/usr/include/HippoMocks/hippomocks.h",
				"${workspaceFolder}/Cplusplus/source/LinkedLists/*.cpp",
				"${workspaceFolder}/Cplusplus/source/NumericProgression/*.cpp",
				"${workspaceFolder}/Cplusplus/source/ProjectConfiguration.cpp",
				"${file}",
				"-l",
				"gtest",
				"-l",
				"gtest_main",
				"-l",
				"gmock",
				"-l",
				"gmock_main",
				"-o",
				"${workspaceFolder}/Cplusplus/bin/${fileBasenameNoExtension}"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": false
			},
			"detail": "compiler: /usr/bin/g++"
		},
		{
			"type": "shell",
			"label": "C/C++: clang build this project in default optimization",
			"command": "/usr/bin/clang++",
			"args": [
				"-g",
				"-Wall",
				"-I",
				"${workspaceFolder}/Cplusplus/Include/",
				"-I",
				"${workspaceFolder}/Cplusplus/Include/LinkedLists/",
				"${workspaceFolder}/Cplusplus/Source/LinkedLists/*.cpp",
				"${workspaceFolder}/Cplusplus/Source/*.cpp",
				"-o",
				"${workspaceFolder}/Cplusplus/bin/project_entry"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": false
			},
			"detail": "compiler: /usr/bin/clang++"
		},
		{
			"type": "shell",
			"label": "Remove build files",
			"command": "rm",
			"args": [
				"-f",
				"Cplusplus/bin/*"
			],
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": []
		}
	]
}